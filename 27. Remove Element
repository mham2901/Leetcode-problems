class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        nums.erase(remove_if(nums.begin(), nums.end(), [val](int x) { return x == val; }), nums.end());
        
        for (int i = 0; i < nums.size(); i++) {
            cout << nums[i] << endl;
        }
        return nums.size();
    }
};
